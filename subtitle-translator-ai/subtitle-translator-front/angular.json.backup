{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "subtitle-translator-front": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/subtitle-translator-front",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "subtitle-translator-front:build",
            "host": "0.0.0.0",
            "port": 4200,
            "allowedHosts": [
              "localhost",
              "127.0.0.1",
              "192.168.0.6",
              "subs.xiltepin.me"
            ]
          },
          "configurations": {
            "production": {
              "buildTarget": "subtitle-translator-front:build:production"
            },
            "development": {
              "buildTarget": "subtitle-translator-front:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": "fe6dfe17-2db7-4561-b85f-61a49938d6f0"
  }
}